<!-- ‚úÖ Script: compute winner, set bars, show only the right section -->
<script>
document.addEventListener("DOMContentLoaded", function () {
  const params = new URLSearchParams(window.location.search);

  let sheep = parseInt(params.get("sheep_score")) || 0;
  let love_bird = parseInt(params.get("love_bird_score")) || 0;
  let owl = parseInt(params.get("owl_score")) || 0;
  let rhino = parseInt(params.get("rhino_score")) || 0;

  // Optional override: ?animal=sheep|love_bird|owl|rhino
  const animalOverride = (params.get("animal") || "").trim();
  if (["sheep", "love_bird", "owl", "rhino"].includes(animalOverride)) {
    sheep     = (animalOverride === "sheep") ? 100 : 0;
    love_bird = (animalOverride === "love_bird") ? 100 : 0;
    owl       = (animalOverride === "owl") ? 100 : 0;
    rhino     = (animalOverride === "rhino") ? 100 : 0;
  }

  const firstName = params.get("first_name") || "Leader";

  // Calculate total & percentages
  const total = sheep + love_bird + owl + rhino;
  const sheepPercent = total ? Math.round((sheep / total) * 100) : 0;
  const love_birdPercent = total ? Math.round((love_bird / total) * 100) : 0;
  const owlPercent = total ? Math.round((owl / total) * 100) : 0;
  const rhinoPercent = total ? Math.round((rhino / total) * 100) : 0;

  // Decide winner with deterministic top-two tiebreak
  const scoresRaw = { sheep, love_bird, owl, rhino };
  const secondaryOrder = ["sheep", "love_bird", "owl", "rhino"];
  const animals = Object.keys(scoresRaw).sort((a,b) => {
    if (scoresRaw[b] !== scoresRaw[a]) return scoresRaw[b] - scoresRaw[a];
    return secondaryOrder.indexOf(a) - secondaryOrder.indexOf(b);
  });
  const topTwo = animals.slice(0,2);
  const topAnimal = (scoresRaw[topTwo[0]] === scoresRaw[topTwo[1]]) ? topTwo[0] : animals[0];

  // Result banner text
  const titles = {
    sheep: "Your personality identifies under the: <span class='highlight-block'>Sheep (The Steadfast)</span>üêë",
    love_bird: "Your personality identifies under the: <span class='highlight-block'>Love Bird (The Connector)</span>ü¶ú",
    owl: "Your personality identifies under the: <span class='highlight-block'>Owl (The Strategist)</span>ü¶â",
    rhino: "Your personality identifies under the: <span class='highlight-block'>Rhino (The Driver)</span>ü¶è"
  };
  const descs = {
    sheep:"The Quiet Anchor. You bring calm, loyalty, and stability into relationships. But avoiding conflict and staying silent too long can slowly create distance instead of peace.",
    love_bird: "The Emotional Pulse. You feel deeply and crave closeness, connection, and reassurance. But when emotions lead without grounding, intensity can overwhelm the very bond you‚Äôre trying to protect.",
    owl: "The Thoughtful Observer. You process deeply and think before you speak, valuing clarity and logic. But staying in your head can leave emotions unmet and your partner feeling alone.",
    rhino: "The Strong Protector. You step up fast when things feel off and take responsibility instinctively. But when strength turns into pressure, it can silence emotions instead of creating safety."
  };

  let resultText = titles[topAnimal];
  let resultDesc = descs[topAnimal];

  // Fallback if all scores = 0
  if (!sheep && !love_bird && !owl && !rhino) {
    resultText = titles.sheep;
   resultDesc ="The Quiet Anchor. You bring calm, loyalty, and stability into relationships. But avoiding conflict and staying silent too long can slowly create distance instead of peace."
;
  }

  document.getElementById("result-title").innerHTML = resultText;
  document.getElementById("result-description").textContent = resultDesc;

  // Greeting
  document.getElementById("greeting").innerHTML =
    `Hi <span class="highlight-name">${firstName}</span>, Here's your personality Breakdownüëá`;

  // CTA buttons ‚Üí go to Page 2 and keep quiz params
  const queryString = params.toString() + "&animal=" + topAnimal;

const cta = document.getElementById("cta-btn");
if (cta) {
  cta.href = "https://biinse-ja.myshopify.com/pages/animal-business-quiz-page-2?" + queryString;
  cta.textContent = "GET YOUR EXTENDED PLAN HERE!";
}

const ctaBottom = document.getElementById("cta-btn-bottom");
if (ctaBottom) {
  ctaBottom.href = "https://biinse-ja.myshopify.com/pages/animal-business-quiz-page-2?" + queryString;
  ctaBottom.textContent = "GET YOUR EXTENDED PLAN HERE!";
}

  // SHOW/HIDE: only show the winning animal section
  document.querySelectorAll(".animal-section").forEach(div => div.style.display = "none");
  const winnerEl = document.getElementById(topAnimal + "-section");
  if (winnerEl) winnerEl.style.display = "block";
});
</script>

